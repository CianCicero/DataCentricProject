<a href="/">Home</a>

<table>

    <tbody>
        <tr>
            <th>SID</th>
            <th>Location</th>
            <th>Manager</th>
            <th>Edit / Delete</th>
        </tr>

        <% stores.forEach(function(store) { %>
        <tr>
            <td><%= store.sid %></td>
            <td><%= store.location %></td>
            <td><%= store.manager %></td>
            <td>
                <a href="/stores/edit/<%= store.sid %>">Edit</a>
                <a href="/stores/delete/<%= store.sid %>">Delete</a>
            </td>
        </tr>
        <% }); %>
    </tbody>

</table>

<a href = "/stores/add">Add Store</a>